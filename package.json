{
  "name": "anytype-runtime",
  "version": "1.0.0",
  "description": "A JavaScript runtime type inference and automatic conversion library",
  "main": "dist/anytype.cjs.js",
  "module": "dist/anytype.esm.js",
  "exports": {
    ".": {
      "import": "./dist/anytype.esm.js",
      "require": "./dist/anytype.cjs.js"
    }
  },
  "browser": "dist/anytype.umd.js",
  "unpkg": "dist/anytype.umd.js",
  "files": [
    "dist",
    "README.md",
    "README_EN.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mxher07/anytype-runtime.git"
  },
  "bugs": {
    "url": "https://github.com/Mxher07/anytype-runtime/issues"
  },
  "homepage": "https://github.com/Mxher07/anytype-runtime#readme",
  "scripts": {
    "build": "rollup -c",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "npm run build:prod"
  },
  "keywords": [
    "types",
    "runtime",
    "inference",
    "javascript",
    "anytype",
    "type-inference",
    "automatic-conversion"
  ],
  "author": "Mxher07",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
